STYLE DS_MODALFRAME | DS_NOIDLEMSG | DS_CENTER | WS_POPUP | WS_DISABLED
CAPTION "Wine"
FONT 8, "MS Shell Dlg"
BEGIN
    ICON "", IDC_WAITICON, 3, 3, 10, 10
    LTEXT "%s にある Wine 設定を更新しています。\nしばらくお待ちください…。",
          IDC_WAITTEXT, 40, 5, 150, 40, SS_NOPREFIX
END
