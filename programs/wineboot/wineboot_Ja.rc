IDD_ENDTASK DIALOG  0, 0, 276, 71
STYLE DS_MODALFRAME | DS_NOIDLEMSG | DS_CENTER | WS_POPUP | WS_CAPTION | WS_SYSMENU
CAPTION "待機中"
FONT 9, "MS Shell Dlg"
BEGIN
    DEFPUSHBUTTON   "キャンセル",IDCANCEL,129,49,50,15
