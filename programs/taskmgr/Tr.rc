    BEGIN
        MENUITEM "Görev Yöneticisi &Yardým Konularý",   ID_HELP_TOPICS
        MENUITEM SEPARATOR
        MENUITEM "Görev Yöneticisi &Hakkýnda",          ID_HELP_ABOUT
    END
END

