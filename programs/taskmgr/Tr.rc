IDR_TASKMANAGER MENU
BEGIN
    POPUP "&Dosya"
    BEGIN
        MENUITEM "&Yeni Görev (Çalýþtýr...)",   ID_FILE_NEW
        MENUITEM SEPARATOR
        MENUITEM "Görev Yöneticisinden &Çýk",   ID_FILE_EXIT
